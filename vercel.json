{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "api/log-to-sheets.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/log-to-sheets",
      "dest": "/api/log-to-sheets.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}